{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../dist/prisma",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node"]
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
