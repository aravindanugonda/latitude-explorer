datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model City {
  id          Int      @id @default(autoincrement())
  name        String
  country     String
  latitude    Float
  longitude   Float
  population  Int?
  timezone    String?
  createdAt   DateTime @default(now())
}